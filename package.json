{
  "name": "yujiali-jinghang-holiday-hotel-management-system",
  "version": "1.0.0",
  "description": "渔家里京杭假日酒店管理系统 - 智能安防、广播管理、数据分析平台",
  "homepage": "https://sdjnzt.github.io/yjljh",
  "main": "public/electron.js",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/plots": "^2.0.1",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "antd": "^5.12.5",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "pinyin-pro": "^3.26.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.0",
    "react-player": "^2.14.1",
    "react-webcam": "^7.2.0",
    "react-hook-form": "^7.48.2",
    "zustand": "^4.4.7",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "js-cookie": "^3.0.5",
    "crypto-js": "^4.2.0",
    "jwt-decode": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.yujiali.jinghang-holiday-hotel",
    "productName": "渔家里京杭假日酒店管理系统",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/electron.js"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icon.png"
    }
  },
  "devDependencies": {
    "gh-pages": "^6.1.0"
  }
}
