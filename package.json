{
  "name": "yujiali-jinghang-holiday-hotel-management-system",
  "version": "1.0.0",
  "description": "渔家里京杭假日酒店管理系统 - 智能安防、广播管理、数据分析平台",
  "homepage": "https://yujiali-hotel.com",
  "main": "public/electron.js",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/plots": "^2.0.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.10.0",
    "@types/pinyin": "^2.10.2",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "antd": "^5.12.5",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "electron-is-dev": "^2.0.0",
    "moment": "^2.30.1",
    "pinyin": "^4.0.0",
    "pinyin-pro": "^3.26.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.0",
    "socket.io-client": "^4.7.4",
    "react-player": "^2.14.1",
    "react-webcam": "^7.2.0",
    "react-sound": "^1.2.0",
    "react-audio-player": "^0.17.0",
    "react-cron": "^1.0.0",
    "react-schedule": "^1.0.0",
    "react-calendar": "^4.6.0",
    "react-time-picker": "^4.5.0",
    "react-date-picker": "^9.1.0",
    "react-select": "^5.8.0",
    "react-table": "^7.8.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "zustand": "^4.4.7",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "js-cookie": "^3.0.5",
    "crypto-js": "^4.2.0",
    "jwt-decode": "^4.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=4096 PORT=5137 react-scripts start",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:5137 && electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --publish=never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.yujiali.jinghang-holiday-hotel",
    "productName": "渔家里京杭假日酒店管理系统",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/electron.js"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icon.png"
    }
  },
  "devDependencies": {
    "@types/echarts": "^4.9.22",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "gh-pages": "^6.1.0",
    "wait-on": "^7.2.0"
  }
}
